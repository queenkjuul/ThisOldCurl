<?xml version="1.0"?>
<package >
  <metadata>
    <id>ThisOldCurl</id>
    <version>0.7.0.0</version>
    <title>ThisOldCurl</title>
    <authors>queenkjuul</authors>
    <owners>queenkjuul</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      ThisOldCurl - The easiest way to use HTTPS on Windows 98 (.NET 2.0)
    </description>
    <tags>curl libcurl http https retro 98 net20</tags>
    <dependencies>
      <group targetFramework=".NETFramework2.0">
      </group>
    </dependencies>
    <readme>README.md</readme>
  </metadata>

  <files>
    <!-- Managed DLL -->
    <file src="bin\Release\ThisOldCurl.dll" target="lib\net20" />

    <!-- Native DLLs -->
    <file src="libcurl.dll" target="runtimes\win-x86\native" />
    <file src="ssleay32.dll" target="runtimes\win-x86\native" />
    <file src="libeay32.dll" target="runtimes\win-x86\native" />

    <file src="ThisOldCurl.targets" target="build\ThisOldCurl.targets" />
    
    <file src="README.md" target="README.md" />

    <file src="cacert.pem" target="build\cacert.pem" />
    <file src="libcurl.dll" target="build\libcurl.dll" />
    <file src="ssleay32.dll" target="build\ssleay32.dll" />
    <file src="libeay32.dll" target="build\libeay32.dll" />
  </files>

</package>
